FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    farfield
    {
        type            zeroGradient;
        value           $internalField;
    }
    lower_inner
    {
        type            zeroGradient;
        value           $internalField;
    }
    upper_inner
    {       
        type            fixedValue;
        value           $internalField;
    }
    "wedge.*"
    {
        type            wedge;
    }
}

